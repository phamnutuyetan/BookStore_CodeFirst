    <script src="~/Content/js/jquery/jquery.scrollUp.min.js"></script>
    <script src="~/Content/js/jquery/jquery.prettyPhoto.js"></script>
    <script src="~/Content/js/main.js"></script>
    <script src="~/Content/js/others/moment.js" type="text/javascript"></script>
    <script src="~/Content/js/helper.js" type="text/javascript"></script>
    <script src="~/Content/js/functions.js" type="text/javascript"></script>
</body>
</html>
